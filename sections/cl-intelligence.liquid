  max-width: 487px;
  width: 100%;
}
.cl_intelligence h4 {
  color: #000433;
  text-align: center;
  font-family: Avenir;
  font-size: 30px;
  font-weight: 800;
  line-height: 36px;
  padding-bottom: 30px;
  margin: 0;
}
.cl_intelligence span.block_decs,
.cl_intelligence span.block_desc li,
.cl_intelligence span.block_desc p {
  color: #515864;
  font-family: Avenir;
  font-size: 26px;
  font-weight: 400;
  line-height: 32px;
  margin: 0;
}
.cl_intelligence span.block_desc li {
  line-height: 80px;
}
.cl_intelligence h5 {
