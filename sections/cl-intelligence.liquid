    padding: 0px;
  }
  .cl_intelligence .logo_image {
    max-width: 100px;
  }
  .cl_intelligence .section_bottom_content {
    margin: 50px auto 0;
  }
  .cl_intelligence .section_top_content {
    gap: 20px;
    flex-direction: column;
  }
  .cl_intelligence .header_image {
    padding-top: 0;
  }
  .cl_intelligence div.desc, 
  .cl_intelligence div.desc p {
    font-size: 16px;
    line-height: 24px;
  }
  .cl_intelligence h3 {
    font-size: 20px;
    line-height: 26px;
    padding-bottom: 20px;
  }
  .cl_intelligence .block_inner {
    gap: 24px;
  }
  .cl_intelligence .block_col {
    padding: 30px 20px;
  }
  .cl_intelligence h4 {
    font-size: 22px;
    line-height: 26px;
    padding-bottom: 10px;
  }
  .cl_intelligence h5 {
      font-size: 16px;
      line-height: 24px;
      padding-top: 15px;
  }
  .cl_intelligence span.block_desc li {
    line-height: 32px;
  }
  .cl_intelligence span.block_decs, 
  .cl_intelligence span.block_desc li, 
  .cl_intelligence span.block_desc p {
    font-size: 18px;
  }
  .cl_intelligence .field__input {
    height: 48px;
  }
  .cl_intelligence .field__button {
    width: 80px !important;
    height: 35px !important;
    font-size: 12px;
    line-height: 18px;
    top: 7px;
  }
  .cl_intelligence .field input::placeholder {
    font-size: 12px;
  }
}
</style>

{%- style -%}
#cl_intelligence-{{ section.id }} {
  background-color: {{ section.settings.bg_color }}; 
  padding-top:{{ section.settings.desktop_padding_top }}px;
  padding-bottom:{{ section.settings.desktop_padding_bottom }}px;
  margin-top:{{ section.settings.desktop_margin_top }}px;
  margin-bottom:{{ section.settings.desktop_margin_bottom }}px;
}
@media screen and (max-width: 991px) {
  #cl_intelligence-{{ section.id }} {
    padding-top:{{ section.settings.mbl_padding_top }}px;
    padding-bottom:{{ section.settings.mbl_padding_bottom }}px;
    margin-top:{{ section.settings.mbl_margin_top }}px;
    margin-bottom:{{ section.settings.mbl_margin_bottom }}px; 
  }
}
{%- endstyle -%}

<section class="cl_intelligence">
    <div class="container" id="cl_intelligence-{{ section.id }}">
      {% if section.settings.logo_img != blank %}
        <div class="logo_image">
          {{ section.settings.logo_img | image_url: width: section.settings.logo_img.width| image_tag: loading: 'lazy'}}
        </div>
      {% endif %}
      {% if section.settings.headline != blank %}
        <h2>{{ section.settings.headline }}</h2>
      {% endif %}
      <div class="section_top_content">
          {% if section.settings.desc != blank %}
            <div class="desc">{{ section.settings.desc }}</div>
          {% endif %}
          {% if section.settings.image != blank %}
            <div class="header_image"> 
              {{ section.settings.image | image_url: width: section.settings.image.width| image_tag: loading: 'lazy'}}
            </div>
          {% endif %}
        </div>

<!-- ********************************************************** Section Bottom Content ************************************************************************* -->
      
        <div class="section_bottom_content">
          {% if section.settings.heading != blank %}
            <h3>{{ section.settings.heading }}</h3>
          {% endif %}
          <div class="block_inner">
             {% for block in section.blocks %} 
              <div class="block_col" id="block_col-{{ block.id }}">
                {% if block.settings.heading != blank %}
                  <h4>{{ block.settings.heading }}</h4>
                {% endif %}
                 {% if block.settings.block_desc != blank %}
                  <span class="block_desc">{{ block.settings.block_desc }}</span>
                {% endif %}
                <div class="email_form">
                  {% if block.settings.input_heading != blank %}
                    <h5>{{ block.settings.input_heading }}</h5>
                  {% endif %}
                   {% form 'customer', class: 'newsletter-form' %}
                      <input type="hidden" name="contact[tags]" value="newsletter">
                        <div class="field">
                          <input
                            id="NewsletterForm--{{ section.id }}"
                            type="email"
                            name="contact[email]"
                            class="field__input"
                            value="{{ form.email }}"
                            aria-required="true"
                            autocorrect="off"
                            autocapitalize="off"
                            autocomplete="email"
                            placeholder="Enter your email..."
                            {% if form.errors %}
                              autofocus
                              aria-invalid="true"
                              aria-describedby="Newsletter-error--{{ section.id }}"
                            {% elsif form.posted_successfully? %}
                              aria-describedby="Newsletter-success--{{ section.id }}"
                            {% endif %}
                            required>
                          <button type="submit"  class="newsletter-form__button field__button" name="commit" id="Subscribe" aria-label="{{ 'newsletter.button_label' | t }}">
                             Notify me
                          </button>
                        {%- if form.errors -%}
                          <small class="newsletter-form__message form__message" id="Newsletter-error--{{ section.id }}">
                            {%- render 'icon-error' -%}
                            {{- form.errors.translated_fields.email | capitalize }}
                            {{ form.errors.messages.email -}}
                          </small>
                        {%- endif -%}
                      </div>
                      {%- if form.posted_successfully? -%}
                        <h3
                          class="newsletter-form__message newsletter-form__message--success form__message"
                          id="Newsletter-success--{{ section.id }}"
                          tabindex="-1"
                          autofocus
                        >
                          {% render 'icon-success' -%}
                          {{- 'newsletter.success' | t }}
                        </h3>
                      {%- endif -%}
                    {% endform %}
                </div>
               </div>
            {% endfor %}
          </div>
        </div>
    </div>
</section>

{% schema %}
  {
    "name": "Cl Intelligence",
    "settings": [
        {
          "type": "image_picker",
          "id": "logo_img",
          "label": "Image"
        },
