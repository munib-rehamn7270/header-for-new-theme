}
.cl_intelligence .newsletter-form {
  max-width: 100% !important;
}
.cl_intelligence .field__input {
  border-radius: 40px;
  border: 1px solid #000433;
  background: #FFF;
  width: 100%;
  height: 55px;
  color: #8D95A3;
  font-family: Avenir;
  font-size: 16px;
  font-weight: 400;
  line-height: normal;
}
.cl_intelligence .field input::placeholder {
  color: #8D95A3;
  font-family: Avenir;
  font-size: 16px;
  font-weight: 400;
  line-height: normal;
  opacity: 1;
  visibility: visible;
}
.cl_intelligence  .field__input:focus {
  box-shadow: unset;
}
.cl_intelligence .field__button {
