    padding-left: 24px;
  }
  .cl_intelligence .logo_image {
    margin-left: 20px;
    margin-bottom: 30px;
  }
  .cl_intelligence h2 {
    font-size: 28px;
    line-height: 36px;
    padding-bottom: 20px;
  }
  .cl_intelligence .section_top_content {
    gap: 20px;
  }
  .cl_intelligence div.desc, 
  .cl_intelligence div.desc p {
    font-size: 18px;
    line-height: 24px;
  }
  .cl_intelligence div.desc strong {
    padding-bottom: 20px;
  }
  .cl_intelligence div.desc ul {
    padding: 25px 0 20px 20px;
  }
  .cl_intelligence .section_bottom_content {
    padding: 40px 20px;
    margin: 80px auto 0;
  }
  .cl_intelligence .block_inner {
    align-items: center;
    flex-direction: column;
  }
   .cl_intelligence .newsletter-form {
    max-width: 100%;
  }
}
@media screen and (max-width: 767px) {
