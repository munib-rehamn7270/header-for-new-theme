    font-size: 24px;
    line-height: 32px;
    padding-bottom: 35px;
  }
  .cl_intelligence .block_inner {
    gap: 40px;
  }
  .cl_intelligence h4 {
    font-size: 24px;
    line-height: 32px;
    padding-bottom: 20px;
  }
