  margin: 0;
}
.cl_intelligence .block_inner {
  display: flex;
  justify-content: center;
  align-items: flex-start;
  gap: 64px;
}
.cl_intelligence .block_col {
  border-radius: 20px;
  background: #F3F5F6;
